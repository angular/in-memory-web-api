    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.36.0",
    "rollup-stream": "^1.14.0",
    "rxjs": "^5.1.0",
    "systemjs": "0.19.40",
    "tslint": "^3.15.1",
    "typescript": "~2.3.2",
    "vinyl-source-stream": "^1.1.0",
