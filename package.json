  "bugs": {
    "url": "https://github.com/angular/in-memory-web-api/issues"
  },
  "homepage": "https://github.com/angular/in-memory-web-api#readme",
  "peerDependencies": {
    "@angular/common": ">=6.0.0 <8.0.0",
    "@angular/core": ">=6.0.0 <8.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@angular/animations": "^7.0.0",
